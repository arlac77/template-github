{
  "template": {
    "inheridFrom": [
      "arlac77/template-root"
    ],
    "usedBy": [
      "arlac77/symatemexplorer",
      "arlac77/template-github-action",
      "k0nsti/konsum",
      "k0nsti/konsum-db"
    ],
    "mergers": [
      {
        "type": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "GitHub Issues",
              "icon": "https://img.shields.io/github/issues/{{github.user}}/{{github.repo}}.svg?style=flat-square",
              "url": "https://github.com/{{github.user}}/{{github.repo}}/issues",
              "order": 0.15
            }
          ]
        }
      }
    ]
  }
}